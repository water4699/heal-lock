    "@types/node": "^24.10.1",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "hardhat": "^2.27.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "@zama-fhe/oracle-solidity": "^0.1.0",
    "dotenv": "^17.2.3",
