{
  "name": "heal-lock",
  "version": "1.0.0",
  "description": "Encrypted Mental Health Diary - Private mental health tracking with FHE",
  "main": "index.js",
  "scripts": {
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "test": "hardhat test",
    "test:sepolia": "hardhat test --network sepolia",
    "deploy": "hardhat deploy",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "deploy:manual": "hardhat run scripts/deploy-manual.ts",
    "deploy:local": "node scripts/deploy-local.js",
    "diagnose": "node scripts/diagnose.js",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "postcompile": "npm run typechain"
  },
  "keywords": [
    "fhevm",
    "zama",
    "mental-health",
    "encryption",
    "blockchain"
